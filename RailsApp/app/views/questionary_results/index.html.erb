<p id="notice"><%= notice %></p>

<h1>Questionary Results</h1>

<table>
  <thead>
    <tr>
      <th>Questionary</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questionary_results.each do |questionary| %>
      <tr>
        <td><%= questionary.id %></td>
        <td><%= qquestionar.title %></td>
        <td><a href="/questionary_results/<%=questionary.id %>">Show</a></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Questionary Result', new_questionary_result_path %>
